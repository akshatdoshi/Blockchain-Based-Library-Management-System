<body>

    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="login-panel panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Please Enter Your Teacher ID</h3>
                    </div>
                    <div class="panel-body">
                        <form method="post" action="/signin">
                            <fieldset>
                                <div class="form-group">
                                    <input class="form-control" placeholder="teacherID" name="teacherID" placeholder="Teacher ID">
                                </div>

                              <button type="submit" class="btn btn-primary btn-lg" id="getresult">Submit</button>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- jQuery -->
    <script src="/../vendor/jquery/jquery.min.js"></script>

